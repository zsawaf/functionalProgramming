datatype ''a tree = Node of ''a * ''a tree list

fun     count(target, Node(data, nil)) = if target=data then 1 else 0
|       count(target, Node(data, (h::t))) = if target=data then 1 + count_helper(target, t) else count_helper(target, t)
and	count_helper(target, nil) = 0
|	count_helper(target, h::t) = count(target, h) + count_helper(target, t);

fun	depth(Node(_, nil)) = 0
|	depth(Node(_, h::t)) = 1 + depth(h) + depth_helper(t)
and	depth_helper(h::t) = depth(h) + depth_helper(t);
